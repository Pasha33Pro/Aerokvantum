.preloader {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: #fff;
  z-index: 1000;
  transition: 1s;
  opacity: 0;
  visibility: hidden;
}

.preloader__logo {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
}

.preloader__logo img {
  display: block;
  max-width: 100%;
}

.preloader svg {
  width: 70px;
  margin-top: 40px;
  animation: preloader-rotate 2s infinite linear;
}

body.load .preloader {
  opacity: 1;
  visibility: visible;
}

@keyframes preloader-rotate {
  100% {
    transform: rotate(360deg);
  }
}


